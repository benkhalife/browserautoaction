[
  {
    "title": "Open URL",
    "type": "goto",
    "url": "https://www.coursera.org/learn/the-psychology-of-emotions?specialization=the-science-of-mind-and-decision-making",
    "sleep": 5
  },
  {
    "type": "click",
    "tag": "button",
    "attr": "data-e2e=",
    "value": "enroll-button",
    "sleep": 10
  },
  {
    "type": "click",
    "tag": "a",
    "attr": "data-click-key",
    "value": "open_course_home.period_page.click.item_link",
    "sleep": 10
  },
  {
    "title": "Find Course List",
    "type": "group_action",
    "tag": "ul>li",
    "actions": [
      {
        "type": "click",
        "tag": "a",
        "sleep": 7
      },
      {
        "type": "scroll",
        "tag": "span",
        "text": "Downloads",
        "ignore": "continue",
        "global": true,
        "sleep": 1
      },
      {
        "title": "Find Download box",
        "type": "click",
        "tag": "span",
        "text": "Downloads",
        "global": true,
        "ignore": "continue",
        "sleep": 4
      },
      {
        "type": "download_from_link",
        "tag": "a",
        "attr": "data-click-key",
        "value": "open_course_home.focused_lex_video_item.click.download_video",
        "text": "Lecture Video (720p)",
        "download_dir": "download_videos",
        "ignore": "continue",
        "global": true,
        "sleep": 5
      }
    ]
  }
]
